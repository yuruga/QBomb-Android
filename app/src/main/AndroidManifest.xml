<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.yuruga.qbomb" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="jp.yuruga.qbomb.QBombApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />

        <activity
            android:name="jp.yuruga.qbomb.AnswerActivity"
            android:label="@string/app_name" >
            <!--<intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>-->
        </activity>
        <activity
            android:name="jp.yuruga.qbomb.QuestionActivity"
            android:label="@string/title_activity_question" >
        </activity>

        <service
            android:name="jp.yuruga.qbomb.GeofenceService"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity
            android:name="jp.yuruga.qbomb.DubugActivity"
            android:label="@string/title_activity_dubug" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
